<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tpmusic">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_music"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_music_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.PlayMusicActivity"></activity>
        <activity android:name=".Activity.DanhSachAllAlbumActivity" />
        <activity android:name=".Activity.DanhSachTheLoaiTheoChuDeActivity" />
        <activity android:name=".Activity.DanhSachAllChuDeActivity" />
        <activity android:name=".Activity.DanhSachPlayListActivity" />
        <activity android:name=".Activity.DanhSachBaiHatActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>